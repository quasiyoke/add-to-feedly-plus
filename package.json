{
  "name": "add-to-feedly-plus",
  "title": "Add to Feedly Plus",
  "description": "Simplifies subscribing to RSS & Atom feeds in Feedly. Browse news feeds and subscribe with a couple of clicks.",
  "author": "Petr Ermishkin <quasiyoke@gmail.com> (https://quasiyoke.me)",
  "contributors": [
    "Saeed Moghadam <phpro.ir@gmail.com>",
    "Yufan Lou <loganlyf@gmail.com>"
  ],
  "license": "MPL 2.0",
  "version": "2.1.3",
  "keywords": [
    "RSS",
    "Feedly"
  ],
  "repository": "https://github.com/quasiyoke/add-to-feedly-plus",
  "bugs": "https://github.com/quasiyoke/add-to-feedly-plus/issues",
  "homepage": "https://quasiyoke.me/en/add-to-feedly-plus/",
  "private": true,
  "scripts": {
    "build": "./etc/build.ts",
    "dev": "chokidar . --command 'npm run build' --ignore dist",
    "check": "npm test && npm run check:types && npm run lint:eslint && npm run check:format && npm run lint:web-ext",
    "test": "vitest run",
    "check:types": "tsc",
    "check:format": "prettier --check .",
    "lint:eslint": "eslint --max-warnings 0 --flag unstable_native_nodejs_ts_config",
    "lint:web-ext": "npm run build && ./etc/lintWebExt.ts"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/addons-linter": "^6.16.1",
    "@types/webextension-polyfill": "^0.12.3",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "addons-linter": "^7.20.0",
    "chokidar-cli": "^3.0.0",
    "chrome-webstore-upload-cli": "^3.3.2",
    "eslint": "^9.33.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^26.1.0",
    "keep-a-changelog": "^2.6.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "web-ext": "^8.9.0",
    "webextension-polyfill": "^0.12.0"
  },
  "type": "module"
}
